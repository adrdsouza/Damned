{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start --port 9000",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.7.1",
    "@medusajs/cache-redis": "^2.7.1",
    "@medusajs/cli": "2.7.1",
    "@medusajs/event-bus-redis": "^2.7.1",
    "@medusajs/framework": "2.7.1",
    "@medusajs/medusa": "2.7.1",
    "@medusajs/utils": "^2.7.1",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "awilix": "^8.0.1",
    "axios": "^1.9.0",
    "bullmq": "^5.52.1",
    "class-transformer": "^0.5.1",
    "dotenv": "^16.5.0",
    "form-data": "^4.0.2",
    "ioredis": "^5.6.1",
    "medusa-payment-nmi": "file:.yalc/medusa-payment-nmi",
    "medusa-payment-sezzle": "file:.yalc/medusa-payment-sezzle",
    "medusa-plugin-nodemailer": "^0.1.4",
    "nodemailer": "^6.10.1",
    "pg": "^8.13.0",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.7.1",
    "@medusajs/types": "^2.7.1",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
